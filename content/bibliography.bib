  pages = {892--901},
  keywords = {Computer Science - Computation and Language,Computer Science - Social and Information Networks,I.2.7,J.4,memory,Physics - Physics and Society,read-ok,recognition,sentence memorability},
  file = {Danescu-Niculescu-Mizil et al_2012_You had me at hello.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/T3WAE5J5/Danescu-Niculescu-Mizil et al_2012_You had me at hello.pdf:application/pdf;arXiv.org Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/DRZA9R5V/1203.html:text/html}
