  date = {2007-12-01},
  pages = {485--507},
  keywords = {Artificial Intelligence (incl. Robotics),Autonomy,Coordination,Enaction,Interaction process,Interdisciplinary Studies,jepaper,making sense,Participatory sense-making,Phenomenology,Philosophy of Mind,read-ok,Sense-making,Social cognition},
  file = {De Jaegher_Di Paolo_2007_Participatory sense-making.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/M4B5KDI7/De Jaegher_Di Paolo_2007_Participatory sense-making.pdf:application/pdf;Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/TE3JIZEI/10.html:text/html}
