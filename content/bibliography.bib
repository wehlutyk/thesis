@article{barrett_spreading_2001,
  title = {Spreading {{Non}}-Natural {{Concepts}}: {{The Role}} of {{Intuitive Conceptual Structures}} in {{Memory}} and {{Transmission}} of {{Cultural Materials}}},
  volume = {1},
  issn = {1568-5373},
  url = {http://booksandjournals.brillonline.com.gate3.inist.fr/content/journals/10.1163/156853701300063589},
  doi = {10.1163/156853701300063589},
  shorttitle = {Spreading {{Non}}-Natural {{Concepts}}},
  abstract = {The four experiments presented support Boyer\&apos;s theory that counterintuitive concepts have transmission advantages that account for the commonness and ease of communicating many non-natural cultural concepts. In Experiment 1, 48 American college students recalled expectation-violating items from culturally unfamiliar folk stories better than more mundane items in the stories. In Experiment 2, 52 American college students in a modified serial reproduction task transmitted expectation-violating items in a written narrative more successfully than bizarre or common items. In Experiments 3 and 4, these findings were replicated with orally presented and transmitted stimuli, and found to persist even after three months. To sum, concepts with single expectation-violating features were more successfully transmitted than concepts that were entirely congruent with category-level expectations, even if they were highly unusual or bizarre. This transmission advantage for counterintuitive concepts may explain, in part, why such concepts are so prevalent across cultures and so readily spread.},
  timestamp = {2017-04-16T11:04:40Z},
  number = {1},
  journaltitle = {Journal of Cognition and Culture},
  author = {Barrett, Justin L. and Nyhof, Melanie A.},
  urldate = {2015-08-31},
  date = {2001-02-01},
  pages = {69--100},
  keywords = {Cultural evolution,Pascal Boyer,read-part,transmission advantage},
  file = {Barrett_Nyhof_2001_Spreading Non-natural Concepts.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/3P9Z7XFE/Barrett_Nyhof_2001_Spreading Non-natural Concepts.pdf:application/pdf;Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/25QS834U/156853701300063589.html:text/html},
