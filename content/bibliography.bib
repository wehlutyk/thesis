@article{mandler_word-frequency_1982,
  title = {The Word-Frequency Paradox in Recognition},
  volume = {10},
  issn = {0090-502X, 1532-5946},
  url = {http://link.springer.com.gate3.inist.fr/article/10.3758/BF03197623},
  doi = {10.3758/BF03197623},
  abstract = {High-frequency words are recalled better than are low-frequency words, but low-frequency words produce higher hit rates in a recognition test than do high-frequency words. Two experiments provided new date on the phenomenon and also evidence relevant to the dual process model of recognition, which postulates that recognition judgments are a function of increments in item familiarity and of item retrievability. First, recall and recognition by subjects who initially performed a single lexical decision task were compared with those of subjects who also gave definitions of high-, low-, and very low-frequency target words. In the second experiment, subjects initially performed either a semantic, elaborative task or an integrative task that focused attention on the physical, perceptual features of the same words. Both experiments showed that extensive elaborative processing results in higher recall and hit rates but lower false alarm rates, whereas word frequency has a monotonic, linear effect on recall and false alarm rates, but a paradoxical, curvilinear effect on hit rates. Elaboration is apparently more effective when the potential availability of meaningful connections with other structures is greater (as for high-frequency words). The results are consistent with the dual process model.},
  timestamp = {2017-04-16T11:05:10Z},
  langid = {english},
  number = {1},
  journaltitle = {Memory \& Cognition},
  shortjournal = {Memory \& Cognition},
  author = {Mandler, George and Goodman, George O. and Wilkes-Gibbs, Deanna L.},
  urldate = {2016-06-30},
  date = {1982},
  pages = {33--42},
  keywords = {memory,word frequency,word recall,word recognition},
  file = {Mandler et al_1982_The word-frequency paradox in recognition.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/NZ6MJVTW/Mandler et al_1982_The word-frequency paradox in recognition.pdf:application/pdf;Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/2PJB7A7H/BF03197623.html:text/html}
}

@article{morrison_roles_1995,
  title = {Roles of Word Frequency and Age of Acquisition in Word Naming and Lexical Decision},
  volume = {21},
  rights = {(c) 2012 APA, all rights reserved},
  issn = {1939-1285(ELECTRONIC);0278-7393(PRINT)},
  doi = {10.1037/0278-7393.21.1.116},
  abstract = {Six experiments are reported that contrasted the effects of frequency and age of acquisition on written word recognition. Age of acquisition affected word-naming speed when frequency was controlled (Experiment 1), but there was no effect of frequency when age of acquisition was controlled (Experiment 2). Experiments 3 and 4 found an effect of age of acquisition upon immediate but not delayed naming speed, but no frequency effect on either immediate or delayed naming once age of acquisition was controlled. Independent effects of frequency and age of acquisition were observed in the lexical decision task (Experiments 5 and 6). Implications for theoretical accounts of word recognition and the possible roles of age of acquisition and frequency in word recognition are discussed.},
  timestamp = {2017-04-16T11:05:10Z},
