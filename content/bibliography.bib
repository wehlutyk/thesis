  author = {Garlock, Victoria M. and Walley, Amanda C. and Metsala, Jamie L.},
  urldate = {2016-06-28},
  date = {2001-10},
  pages = {468--492},
  keywords = {age-of-acquisition,beginning reading,lexical representations,neighborhood density,phonological awareness,read-abstract,spoken word recognition,vocabulary growth,word frequency.},
  file = {Garlock et al_2001_Age-of-Acquisition, Word Frequency, and Neighborhood Density Effects on Spoken.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/7DI495UU/Garlock et al_2001_Age-of-Acquisition, Word Frequency, and Neighborhood Density Effects on Spoken.pdf:application/pdf;ScienceDirect Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/TZB2I4NB/S0749596X00927840.html:text/html}
