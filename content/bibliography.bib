  urldate = {2016-06-28},
  date = {2002-01},
  pages = {85--98},
  keywords = {episodic retrieval,free recall,latent semantic analysis.,LSA,read-part,semantic similarity,topic model,word recall},
  file = {Howard_Kahana_2002_When Does Semantic Similarity Help Episodic Retrieval.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/85KKV48E/Howard_Kahana_2002_When Does Semantic Similarity Help Episodic Retrieval.pdf:application/pdf;ScienceDirect Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/XJBHS88R/S0749596X01927986.html:text/html}
