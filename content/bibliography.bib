@article{potter_syntactic_1998,
  title = {Syntactic {{Priming}} in {{Immediate Recall}} of {{Sentences}}},
  volume = {38},
  issn = {0749-596X},
  url = {http://www.sciencedirect.com/science/article/pii/S0749596X97925468},
  doi = {10.1006/jmla.1997.2546},
  abstract = {In two previous papers (Lombardi \&amp; Potter, 1992; Potter \&amp; Lombardi, 1990) we reported evidence that immediate recall of a sentence requires regeneration from the message level, rather than from a verbatim representation. However, participants tended to reproduce the surface syntax even when there were two meaning-equivalent surface structures available (e.g., for dative verbs, “gave the letter to her mother,” “gave her mother the letter”). In three experiments we tested the hypothesis that this verbatim bias is the result of syntactic priming (Bock, 1986). In Experiment 1 single sentences were recalled; the prime sentence preceded the target dative sentence. In Experiments 2 and 3 two-clause sentences were recalled; the second clause served as a prime that had been perceived but not yet recalled when the first clause was produced, or vice versa. When the prime sentence or clause was a dative that mismatched the surface structure of the target there was an increase in changes to the alternate (primed) structure in recall of the target, compared with control primes. These results support the hypothesis that simply perceiving a sentence is enough to prime its surface syntactic structure, contributing to verbatim recall.},
  timestamp = {2017-04-16T11:04:08Z},
  number = {3},
  journaltitle = {Journal of Memory and Language},
  shortjournal = {Journal of Memory and Language},
  author = {Potter, Mary C. and Lombardi, Linda},
  urldate = {2015-07-30},
  date = {1998-04},
  pages = {265--282},
  keywords = {read-abstract,sentence recall,syntactic priming,syntax regeneration},
  file = {Potter_Lombardi_1998_Syntactic Priming in Immediate Recall of Sentences.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/ZFT2I73D/Potter_Lombardi_1998_Syntactic Priming in Immediate Recall of Sentences.pdf:application/pdf;ScienceDirect Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/AEPTXFXA/S0749596X97925468.html:text/html}
}

@article{yonelinas_nature_2002,
  title = {The {{Nature}} of {{Recollection}} and {{Familiarity}}: {{A Review}} of 30 {{Years}} of {{Research}}},
  volume = {46},
  issn = {0749-596X},
  url = {http://www.sciencedirect.com/science/article/pii/S0749596X02928640},
  doi = {10.1006/jmla.2002.2864},
  shorttitle = {The {{Nature}} of {{Recollection}} and {{Familiarity}}},
  abstract = {To account for dissociations observed in recognition memory tests, several dual-process models have been proposed that assume that recognition judgments can be based on the recollection of details about previous events or on the assessment of stimulus familiarity. In the current article, these models are examined, along with the methods that have been developed to measure recollection and familiarity. The relevant empirical literature from behavioral, neuropsychological, and neuroimaging studies is then reviewed in order to assess model predictions. Results from a variety of measurement methods, including task-dissociation and process-estimation methods, are found to lead to remarkably consistent conclusions about the nature of recollection and familiarity, particularly when ceiling effects are avoided. For example, recollection is found to be more sensitive than familiarity to response speeding, division of attention, generation, semantic encoding, the effects of aging, and the amnestic effects of benzodiazepines, but it is less sensitive than familiarity to shifts in response criterion, fluency manipulations, forgetting over short retention intervals, and some perceptual manipulations. Moreover, neuropsychological and neuroimaging results indicate that the two processes rely on partially distinct neural substrates and provide support for models that assume that recollection relies on the hippocampus and prefrontal cortex, whereas familiarity relies on regions surrounding the hippocampus. Double dissociations produced by experimental manipulations at time of test indicate that the two processes are independent at retrieval, and single dissociations produced by study manipulations indicate that they are partially independent during encoding. Recollection is similar but not identical to free recall, whereas familiarity is similar to conceptual implicit memory, but is dissociable from perceptual implicit memory. Finally, the results indicate that recollection reflects a thresholdlike retrieval process that supports novel learning, whereas familiarity reflects a signal-detection process that can support novel learning only under certain conditions. The results verify a number of model predictions and prove useful in resolving several theoretical disagreements.},
  timestamp = {2017-04-16T11:04:22Z},
  number = {3},
  journaltitle = {Journal of Memory and Language},
  shortjournal = {Journal of Memory and Language},
  author = {Yonelinas, Andrew P},
  urldate = {2014-12-12},
  date = {2002-04},
  pages = {441--517},
  keywords = {linguistics,memory,psycholinguistics,word familiarty,word frequency,word recall},
  file = {Yonelinas_2002_The Nature of Recollection and Familiarity.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/QWT6D5TH/Yonelinas_2002_The Nature of Recollection and Familiarity.pdf:application/pdf;ScienceDirect Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/B9GN2XQH/S0749596X02928640.html:text/html}
}

@article{claidiere_cultural_2014,
  title = {Cultural Evolution of Systematically Structured Behaviour in a Non-Human Primate},
  volume = {281},
  rights = {. © 2014 The Authors. Published by the Royal Society under the terms of the Creative Commons Attribution License http://creativecommons.org/licenses/by/4.0/, which permits unrestricted use, provided the original author and source are credited.},
  issn = {0962-8452, 1471-2954},
  url = {http://rspb.royalsocietypublishing.org/content/281/1797/20141541},
  doi = {10.1098/rspb.2014.1541},
  abstract = {Culture pervades human life and is at the origin of the success of our species. A wide range of other animals have culture too, but often in a limited form that does not complexify through the gradual accumulation of innovations. We developed a new paradigm to study cultural evolution in primates in order to better evaluate our closest relatives' cultural capacities. Previous studies using transmission chain experimental paradigms, in which the behavioural output of one individual becomes the target behaviour for the next individual in the chain, show that cultural transmission can lead to the progressive emergence of systematically structured behaviours in humans. Inspired by this work, we combined a pattern reproduction task on touch screens with an iterated learning procedure to develop transmission chains of baboons (Papio papio). Using this procedure, we show that baboons can exhibit three fundamental aspects of human cultural evolution: a progressive increase in performance, the emergence of systematic structure and the presence of lineage specificity. Our results shed new light on human uniqueness: we share with our closest relatives essential capacities to produce human-like cultural evolution.},
  timestamp = {2017-04-16T11:04:23Z},
  langid = {english},
  number = {1797},
  journaltitle = {Proceedings of the Royal Society of London B: Biological Sciences},
  author = {Claidière, Nicolas and Smith, Kenny and Kirby, Simon and Fagot, Joël},
  urldate = {2015-03-31},
  date = {2014-12-22},
  pages = {20141541},
  keywords = {Cultural attraction,Cultural evolution,epidemiology of representations,non-human primates,read-ok},
  file = {Claidière et al_2014_Cultural evolution of systematically structured behaviour in a non-human primate.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/I96WS9BU/Claidière et al_2014_Cultural evolution of systematically structured behaviour in a non-human primate.pdf:application/pdf;Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/3AMJSFKA/20141541.html:text/html}
}

@article{ingold_complementarity_1998,
  title = {From {{Complementarity}} to {{Obviation}}: {{On Dissolving}} the {{Boundaries Between Social}} and {{Biological Anthropology}}, {{Archaeology}} and {{Psychology}}},
  volume = {123},
  rights = {Copyright © 1998 Dietrich Reimer Verlag GmbH},
  issn = {0044-2666},
  shorttitle = {From Complementarity to Obviation},
  abstract = {The split between the humanities and the natural sciences, and within anthropology between its sociocultural and biological divisions, has its source in a notion of the human being as a composite entity made up of separate but complementary parts, related as (innate) container to (acquired) content. This notion, however, precludes an adequate account of ontogenetic development. An alternative approach, based on a principle of obviation rather than complementarity, sets out to dissolve the boundaries by which the biological, psychological and social components of human being have conventionally been distinguished. In this approach, which takes as its starting point the notion of the organism-person as a locus of growth within a field of relationships, human capacities are explained as the properties not of genetic or cultural programming but of the self-organising dynamics of developmental systems. It is possible, then, to dispense not only with the biological/social dichotomy but also with that between evolution and history. This has important implications for archaeology, with regard to the issues both of human origins and landscape formation. Conceived as the study of environments in their temporal unfolding, archaeology could be regarded as the obverse of the anthropological study of the conditions of human being-in-the-world. In psychology, too, the complementarity thesis is unable to offer a convincing account of development. From the perspective of an obviation approach, the phenomena of perception, memory and learning could just as well be treated as topics of anthropological inquiry, at the same time opening up a new perspective on the study of children. Finally, if anthropology is a science, it must be a science of engagement. This conclusion has implications for teaching as well as research. Die Spaltung zwischen den Human- und Naturwissenschaften, und innerhalb der Anthropologie zwischen deren sozio-kulturellen und biologischen Richtungen, hat ihre Grundlage in der Auffassung des Menschen als aus zwei verschiedenen, aber sich ergänzenden Teilen zusammengefügter Entität — als Gefäß, dessen Form angeboren ist, mit einem Inhalt, der erworben wird. Diese Begriffsbildung schließt eine angemessene Darstellung der ontogenetischen Entwicklung aus. Ein alternativer Ansatz, der eher auf Aufhebung als Ergänzung beruht, zielt darauf ab, die Grenzen aufzulösen, durch welche die biotischen, psychischen und sozialen Komponenten des Menschen herkömmlicherweise unterschieden wurden. Bei diesem Ansatz, der die Person als Organismus, als einen Ort des Wachsens in einem Feld der Beziehungen zu seinem Ausgangspunkt nimmt, werden menschlichen Fähigkeiten als Eigenheiten nicht der genetischen oder kulturellen Programmierung angesehen, sondern der sich selbst organisierenden Dynamik von Entwicklungssystemen. So wird nicht nur die Dichotomie biologisch/sozial, sondern auch diejenige zwischen Evolution und Geschichte überflüssig. Dies hat im Hinblick auf beides, die Anfänge der Menschheit und die Landschaftsbildung, wichtige Implikationen für die Archäologie. Da sie vom Einfluß der jeweiligen Umwelt in ihrer zeitlichen Entfaltung ausgeht, kann die Archäologie als das Gegenstück zur Anthropologie angesehen werden, die vom Menschen in der Welt ausgeht. Auch für die Psychologie kann die These der Ergänzung keine überzeugende Einschätzung von Entwicklungen anbieten. Bei Aufhebung der Dichotomie jedoch könnten die Phänomene der Perzeption, des Gedächtnisses und Lernens genau so gut behandelt werden wie die Gegenstände anthropologischer Fragestellungen, und sie eröffnete zugleich eine neue Perspektive auf die Untersuchung von Kindern. Schließlich muß Anthropologie auch eine Wissenschaft des Engagements sein, sowohl in Forschung wie in Lehre.},
  timestamp = {2017-04-16T11:04:24Z},
  eprinttype = {jstor},
  eprint = {25842543},
  number = {1},
  journaltitle = {Zeitschrift für Ethnologie},
  shortjournal = {Zeitschrift für Ethnologie},
  author = {Ingold, Tim},
  date = {1998-01-01},
  pages = {21--52},
  keywords = {Biological Evolution,cultural epidemiology,Cultural evolution,developmental system,ecological psycholoy,evolution,evolutionary psychology,sensorimotor approach},
  file = {Ingold_1998_From Complementarity to Obviation.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/8GUPPQSC/Ingold_1998_From Complementarity to Obviation.pdf:application/pdf}
}

@article{mesoudi_towards_2006,
  title = {Towards a Unified Science of Cultural Evolution},
  volume = {29},
  issn = {1469-1825},
  url = {http://journals.cambridge.org/article_S0140525X06009083},
  doi = {10.1017/S0140525X06009083},
  abstract = {We suggest that human culture exhibits key Darwinian evolutionary properties, and argue that the structure of a science of cultural evolution should share fundamental features with the structure of the science of biological evolution. This latter claim is tested by outlining the methods and approaches employed by the principal subdisciplines of evolutionary biology and assessing whether there is an existing or potential corresponding approach to the study of cultural evolution. Existing approaches within anthropology and archaeology demonstrate a good match with the macroevolutionary methods of systematics, paleobiology, and biogeography, whereas mathematical models derived from population genetics have been successfully developed to study cultural microevolution. Much potential exists for experimental simulations and field studies of cultural microevolution, where there are opportunities to borrow further methods and hypotheses from biology. Potential also exists for the cultural equivalent of molecular genetics in “social cognitive neuroscience,” although many fundamental issues have yet to be resolved. It is argued that studying culture within a unifying evolutionary framework has the potential to integrate a number of separate disciplines within the social sciences.},
  timestamp = {2017-04-16T11:04:25Z},
  issue = {04},
  journaltitle = {Behavioral and Brain Sciences},
  author = {Mesoudi, Alex and Whiten, Andrew and Laland, Kevin N.},
  urldate = {2015-02-13},
  date = {2006-08},
  pages = {329--347},
  keywords = {_tablet,Biological Evolution,Cultural evolution},
  file = {Mesoudi et al_2006_Towards a unified science of cultural evolution.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/X7R2283Z/Mesoudi et al_2006_Towards a unified science of cultural evolution.pdf:application/pdf;Cambridge Journals Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/IVG3ZJ7S/displayAbstract.html:text/html},
  groups = {EHBEA CAT}
