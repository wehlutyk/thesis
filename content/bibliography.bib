  date = {2010-05-01},
  pages = {685--697},
  keywords = {clustering coefficient,Cognitive Architecture,complex systems,Computer Science,linguistics,Memory,Psychology,read-ok},
  file = {Chan_Vitevitch_2010_Network Structure Influences Speech Production.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/36WJ6QSP/Chan_Vitevitch_2010_Network Structure Influences Speech Production.pdf:application/pdf;Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/EUCPXAIB/full.html:text/html}
