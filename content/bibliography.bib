  pages = {1--29},
  keywords = {age of acquisition,connectionist modeling,linguistic development.,reading},
  file = {Zevin_Seidenberg_2002_Age of Acquisition Effects in Word Reading and Other Tasks.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/36S7RMHV/Zevin_Seidenberg_2002_Age of Acquisition Effects in Word Reading and Other Tasks.pdf:application/pdf;ScienceDirect Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/NA77MMMX/S0749596X01928347.html:text/html}
