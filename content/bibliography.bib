  keywords = {Computer Science - Computation and Language,Computer Science - Social and Information Networks,I.2.7,Physics - Physics and Society},
  file = {Danescu-Niculescu-Mizil et al_2013_A Computational Approach to Politeness with Application to Social Factors.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/9QGIENGN/Danescu-Niculescu-Mizil et al_2013_A Computational Approach to Politeness with Application to Social Factors.pdf:application/pdf;arXiv.org Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/I79TAI3P/1306.html:text/html}
