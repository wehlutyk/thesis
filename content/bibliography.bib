  pages = {775--793},
  keywords = {Iterated learning,language evolution,Linguistic structure},
  file = {Perfors_Navarro_2014_Language Evolution Can Be Shaped by the Structure of the World.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/CKD2Z8Z9/Perfors_Navarro_2014_Language Evolution Can Be Shaped by the Structure of the World.pdf:application/pdf;Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/ANJAR6XZ/abstract.html:text/html},
