  keywords = {Cultural evolution,epidemiology of representations,Iterated learning,language evolution,read-ok},
  file = {Kirby et al_2008_Cumulative cultural evolution in the laboratory.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/RM7K7E99/Kirby et al_2008_Cumulative cultural evolution in the laboratory.pdf:application/pdf;Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/XRC3829Q/10681.html:text/html},
