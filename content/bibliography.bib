  urldate = {2016-06-28},
  date = {2007-12-01},
  pages = {1069--1076},
  keywords = {pagerank,read-ok,word association},
  file = {Griffiths et al_2007_Google and the Mind Predicting Fluency With PageRank.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/NF32IZXM/Griffiths et al_2007_Google and the Mind Predicting Fluency With PageRank.pdf:application/pdf;Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/R352QKAG/1069.html:text/html}
