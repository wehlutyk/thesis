  author = {Kuperman, Victor and Stadthagen-Gonzalez, Hans and Brysbaert, Marc},
  urldate = {2016-06-30},
  date = {2012-05-12},
  pages = {978--990},
  keywords = {age of acquisition,word features},
  file = {Kuperman et al_2012_Age-of-acquisition ratings for 30,000 English words.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/MP5JQDAT/Kuperman et al_2012_Age-of-acquisition ratings for 30,000 English words.pdf:application/pdf;Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/JE5FBU6R/s13428-012-0210-4.html:text/html}
