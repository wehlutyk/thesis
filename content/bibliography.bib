  urldate = {2017-01-16},
  date = {2007-04-01},
  pages = {288--294},
  keywords = {Cultural evolution,epidemiology of representations},
  file = {Kalish et al_2007_Iterated learning.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/BZSE6WES/Kalish et al_2007_Iterated learning.pdf:application/pdf;Snapshot:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/QF8MCIMT/BF03194066.html:text/html},
  groups = {Cultural Transmission,EHBEA CAT}
