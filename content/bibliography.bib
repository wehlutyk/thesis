  author = {Thompson, Evan},
  date = {2007},
  keywords = {enactivism,jepaper,read-next},
  file = {Thompson_2007_Mind in Life.pdf:/home/sl/.zotero/zotero/79j6f8rc.default/zotero/storage/RDNBRQGE/Thompson_2007_Mind in Life.pdf:application/pdf}
}

@article{sterelny_minds:_2010,
